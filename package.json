{
  "name": "ReactNativeJest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "codegen": "graphql-codegen-esm --config codegen.ts",
    "test:e2e": "maestro test src/.maestro/flow.yaml",
    "podNewClear": "cd ios && rm -rf Pods/* && rm -rf Podfile.lock && cd ..",
    "android:installUAT": "cd android && ./gradlew clean && ./gradlew installReleaseUAT",
    "android:installSIT": "cd android && ./gradlew clean && ./gradlew installReleaseSIT"
  },
  "dependencies": {
    "@apollo/client": "^3.9.5",
    "@graphql-codegen/cli": "^5.0.2",
    "@react-native-google-signin/google-signin": "^11.0.0",
    "@react-navigation/native": "^6.1.14",
    "@react-navigation/stack": "^6.3.25",
    "@testing-library/jest-native": "^5.4.3",
    "axios": "^1.6.7",
    "graphql": "^16.8.1",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-app-auth": "^7.1.3",
    "react-native-config": "^1.5.1",
    "react-native-fbsdk-next": "^12.1.4",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screen": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@testing-library/react-native": "^12.4.3",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "^18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
